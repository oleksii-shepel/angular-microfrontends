{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"cli": {
		"analytics": false
	},
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"shared": {
			"projectType": "library",
			"root": "projects/shared",
			"sourceRoot": "projects/shared/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/shared/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/shared/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/shared/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/shared/src/test.ts",
						"tsConfig": "projects/shared/tsconfig.spec.json",
						"karmaConfig": "projects/shared/karma.conf.js"
					}
				}
			}
		},
		"navbar": {
			"projectType": "library",
			"root": "projects/navbar",
			"sourceRoot": "projects/navbar/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/navbar/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/navbar/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/navbar/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/navbar/src/test.ts",
						"tsConfig": "projects/navbar/tsconfig.spec.json",
						"karmaConfig": "projects/navbar/karma.conf.js"
					}
				}
			}
		},
		"store": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"inlineStyle": true,
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:resolver": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/store",
			"sourceRoot": "projects/store/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/store",
						"index": "projects/store/src/index.html",
						"main": "projects/store/src/main.ts",
						"polyfills": "projects/store/src/polyfills.ts",
						"tsConfig": "projects/store/tsconfig.app.json",
						"assets": [
							"projects/store/src/favicon.ico",
							"projects/store/src/assets"
						],
						"styles": [
							"projects/store/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/store/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/store/src/environments/environment.ts",
									"with": "projects/store/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/store/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "store:build:production",
							"extraWebpackConfig": "projects/store/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "store:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4202,
						"publicHost": "http://localhost:4202",
						"extraWebpackConfig": "projects/store/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "store:build",
						"extraWebpackConfig": "projects/store/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/store/src/test.ts",
						"polyfills": "projects/store/src/polyfills.ts",
						"tsConfig": "projects/store/tsconfig.spec.json",
						"karmaConfig": "projects/store/karma.conf.js",
						"assets": [
							"projects/store/src/favicon.ico",
							"projects/store/src/assets"
						],
						"styles": [
							"projects/store/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
